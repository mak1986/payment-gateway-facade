{
	"detect_order":[
		"visa_electron", 
		"maestro", 
		"dankort", 
		"interpayment",
		"unionpay",
		"visa",
		"mastercard",
		"amex",
		"diners_club",
		"discover",
		"jcb"
	],
	"credit_card_types":{
		"amex": {
			"machine_name": "amex",
			"pretty_name": "American Express",
			"regex": "^3[47][0-9]{13}$"
		},
		"dankort": {
			"machine_name": "dankort",
			"pretty_name": "Dankort",
			"regex": "^(5019)\\d+$"
		},
		"diners_club": {
			"machine_name": "diners_club",
			"pretty_name": "Diners Club",
			"regex": "^3(?:0[0-5]|[68][0-9])[0-9]{11}$"
		},
		"discover": {
			"machine_name": "discover",
			"pretty_name": "Discover",
			"regex": "^6(?:011|5[0-9]{2})[0-9]{12}$"
		},
		"interpayment":{
			"machine_name": "interpayment",
			"pretty_name": "Interpayment",
			"regex": "^(636)\\d+$"
		},
		"jcb": {
			"machine_name": "jcb",
			"pretty_name": "JCB",
			"regex": "^(?:2131|1800|35\\d{3})\\d{11}$"
		},
		"maestro": {
			"machine_name": "maestro",
			"pretty_name": "Maestro",
			"regex": "^(5018|5020|5038|5612|5893|6304|6759|6761|6762|6763|0604|6390)\\d+$"
		},
		"mastercard": {
			"machine_name": "mastercard",
			"pretty_name": "MasterCard",
			"regex": "^5[1-5][0-9]{14}$"
		},
		"unionpay": {
			"machine_name": "unionpay",
			"pretty_name": "Unionpay",
			"regex": "^(62|88)\\d+$"
		},
		"visa": {
			"machine_name": "visa",
			"pretty_name": "Visa",
			"regex": "^4[0-9]{12}(?:[0-9]{3})?$"
		},
		"visa_electron": {
			"machine_name": "visa_electron",
			"pretty_name" : "Visa Electron",
			"regex": "^(4026|417500|4405|4508|4844|4913|4917)\\d+$"
		}
	}
}